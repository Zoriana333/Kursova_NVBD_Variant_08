  <MdxScripts>
    <MdxScript dwd:design-time-name="dfd32449-af5f-4a19-a6c0-0e60301612e1">
      <ID>MdxScript</ID>
      <Name>MdxScript</Name>
      <CreatedTimestamp>0001-01-01T00:00:00Z</CreatedTimestamp>
      <LastSchemaUpdate>0001-01-01T00:00:00Z</LastSchemaUpdate>
      <Commands>
        <Command>
          <Text>CALCULATE;
CREATE MEMBER CURRENTCUBE.[Measures].[Average Price per Unit]
 AS IIF(
    [Measures].[Qty Sold] = 0,
    NULL,
    [Measures].[Sales Amount] / [Measures].[Qty Sold]
), 
FORMAT_STRING = "Currency", 
VISIBLE = 1 ,  ASSOCIATED_MEASURE_GROUP = 'Fact Sales';  
CREATE MEMBER CURRENTCUBE.[Measures].[Sales YTD]
 AS Aggregate(
  YTD([Time dimension].[Calendar].CurrentMember),
  [Measures].[Sales Amount]
), 
FORMAT_STRING = "Currency", 
VISIBLE = 1 ,  ASSOCIATED_MEASURE_GROUP = 'Fact Sales';  
CREATE MEMBER CURRENTCUBE.[Measures].[Sales PY]
 AS (
  [Measures].[Sales Amount],
  ParallelPeriod(
    [Time dimension].[Calendar].[Year],
    1,
    [Time dimension].[Calendar].CurrentMember
  )
), 
FORMAT_STRING = "Currency", 
VISIBLE = 1 ,  ASSOCIATED_MEASURE_GROUP = 'Fact Sales';  
CREATE MEMBER CURRENTCUBE.[Measures].[Sales YoY %]
 AS IIF(
  [Measures].[Sales PY] = 0 OR IsEmpty([Measures].[Sales PY]),
  NULL,
  ([Measures].[Sales Amount] - [Measures].[Sales PY]) / [Measures].[Sales PY]
), 
FORMAT_STRING = "Percent", 
VISIBLE = 1 ,  ASSOCIATED_MEASURE_GROUP = 'Fact Sales';  
CREATE MEMBER CURRENTCUBE.[Measures].[Sales Moving Avg 3]
 AS Avg(
  LastPeriods(3, [Time dimension].[Calendar].CurrentMember),
  [Measures].[Sales Amount]
), 
FORMAT_STRING = "Currency", 
VISIBLE = 1 ,  ASSOCIATED_MEASURE_GROUP = 'Fact Sales';  
CREATE MEMBER CURRENTCUBE.[Measures].[Product Rank by Sales]
 AS Rank(
  [Product dimension].[Product Hierarchy].CurrentMember,
  Order(
    [Product dimension].[Product Hierarchy].[Product Name].Members,
    [Measures].[Sales Amount],
    BDESC
  )
), 
FORMAT_STRING = "Standard", 
VISIBLE = 1 ,  ASSOCIATED_MEASURE_GROUP = 'Fact Sales'; </Text>
        </Command>
      </Commands>
    </MdxScript>
  </MdxScripts>
